
#include <list>
using namespace std;

void vec(int n)
{
    list<int> mylist = { 1, 3, 4, 5, 33, 55, 66, 0 };

    for (list<int>::iterator it = mylist.begin(); it != mylist.end(); )
    {
        if (*it < n)
        {
            it = mylist.erase(it);
        }
        else
        {
            cout << *it << " ";
            ++it;
        }
    }
}


int main()
{
   vec(4);
}
